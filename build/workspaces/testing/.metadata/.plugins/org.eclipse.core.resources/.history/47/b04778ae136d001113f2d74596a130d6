package de.gebit.integrity.tests.junit.basic;

import static org.junit.Assert.*;

import org.junit.Test;

import de.gebit.integrity.tests.junit.IntegrityJUnitTest;

public class SimpleFixtureCalls extends IntegrityJUnitTest {

	@Test
	public void test() {
		IntegrityExecutionResult tempResult = executeIntegritySuite("integrity.basic.simpleFixtureCalls");
		assertNull(tempResult.getException());
		assertNotNull(tempResult.getStandardOutput());
		assertNotNull(tempResult.getStandardError());
		assertNotNull(tempResult.getXmlResult());
	}

}
